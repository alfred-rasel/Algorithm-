

### 
#include<bits/stdc++.h> 
using namespace std;
int power(int x, unsigned y,unsigned M)
{
    if(y==0) return 1;
    int p=power(x,y/2,M)%M;
    p=(p*p)%M;
    return (y%2==0) ? p:(p*x)%M;

}

int MMI(int a,int M)
{
    // m prime number 
   
   int ans= power(a,M-2,M);
 return ans;

}
int findx(int num[],int rem[],int n)
{
    int i,prod=1,pp, x;
    for(i=0;i<n;i++)
    {
        prod*=num[i];

    }
    for(i=0;i<n;i++)
    {
        pp=prod/num[i];
        x+=rem[i]*pp*MMI(pp,num[i]);

    }
    return x;
}
int main()
{
    int i, n=3;
    int num[n];
   int  rem[n];
    for(i=0;i<n;i++)
    {
        cin>>num[i];

    }
      for(i=0;i<n;i++)
    {
        cin>>rem[i];

    }
    int final=findx(num,rem,n);
    cout<<final<<endl;

}
