#include<bits/stdc++.h> 
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        //check korbo n odd or even 
        //odd hole n ta 1 hobe 
        if(n%2!=0)
        {
            for(int i=1;i<=n;i++)
            {
                cout<<"1 ";
            }
            cout<<endl;
        }
        else{
            //even hole n-2 ta 
            // 2 akta 1 akta 3 
            for(int i=1;i<=n-2;i++)
            {
                cout<<"2 ";
            }
            cout<<"1 ";
            cout<<"3 "<<endl;
        }
    }
}
